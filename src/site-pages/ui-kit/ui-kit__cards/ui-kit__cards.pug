section.ui-kit__cards
  include ../../../cards/find-room-card/find-room-card
  +findRoomCard()(addClasses = "ui-kit__find-room-card")
  include ../../../cards/booking-card/booking-card
  -var customers = [{name: "Взрослые", value: 3}, {name: "Дети", value: 0}, {name: "Младенцы", value: 0}]
  +bookingCard({
    roomNumber: "888",
    roomType: "люкс",
    currency: "₽",
    dailyPrice: 9990,
    arrivalDate: "19.08.2019",
    departureDate: "23.08.2019",
    services: [{name: "скидка", cost: -2179}],
    additionalPrice: 300,
    dropdownItems: customers
  })(addClasses = "ui-kit__booking-card")
  include ../../../blocks/input/input
  +input("", "datepicker medium inline", "ДД.ММ.ГГГГ", "ui-kit__datepicker_inline", "", ["19.08.2019", "23.08.2019"])
  include ../../../cards/registration-card/registration-card
  +registrationCard()(addClasses = "ui-kit__registration-card")
  include ../../../cards/login-card/login-card
  +loginCard()(addClasses = "ui-kit__login-card")
  include ../../../cards/room-preview-card/room-preview-card
  +roomPreviewCard(
    {
      roomNumber: "888",
      roomType: "люкс",
      costPerPeriod: 9990,
      currencySymbol: "₽",
      periodText: "в сутки",
      reviewsCount: "145",
      rating: [5, 5],
    },
    {
      content: [
        {
          tag: "img",
          src: require("../../assets/images/room-mini-1.jpg")
        },
        {
          tag: "img",
          src: require("../../assets/images/room-mini-2.jpg")
        },
        {
          tag: "img",
          src: require("../../assets/images/room-mini-1.jpg")
        },
        {
          tag: "img",
          src: require("../../assets/images/room-mini-2.jpg")
        },],
      carouselParams: {
        "data-arrows": "true",
        "data-dots": "true",
      },
    }
  )(addClasses = "ui-kit__room-preview-card")
  +roomPreviewCard(
    {
      roomNumber: "840",
      costPerPeriod: 9990,
      currencySymbol: "₽",
      periodText: "в сутки",
      reviewsCount: "65",
      rating: [4, 5],
    },
    {
      content: [
        {
          tag: "img",
          src: require("../../assets/images/room-mini-2.jpg")
        },
        {
          tag: "img",
          src: require("../../assets/images/room-mini-1.jpg")
        },
        {
          tag: "img",
          src: require("../../assets/images/room-mini-2.jpg")
        },
        {
          tag: "img",
          src: require("../../assets/images/room-mini-1.jpg")
        },],
      carouselParams: {
        "data-arrows": "false",
        "data-dots": "true",
      },
    }
  )(addClasses = "ui-kit__room-preview-card_dots-only")
